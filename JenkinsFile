
pipeline {
    agent any
    stages {
        stage('SCM Checkout') {
            steps {
                echo "Source Code Gathering..."
            }
        }
        
        stage('Prepare') {
            steps {
                echo "installing..."
            }
        }
        stage('Package') {
            steps {
                echo "Building..."
            }
        }
        stage('Unit Test') {
            steps {
                echo "Testing..."
            }
        }
        stage('Publish') {
            steps {
                echo "Deploying..."
            }
        }
    }
}