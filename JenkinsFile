pipeline {
  agent any
    
  tools {nodejs "WJAR_JS_Proj"}
    
  stages {
        
    stage('Cloning Git') {
      steps {
        git 'https://github.com/WJosueA/Rock-Paper-Scissors-JS'
      }
    }
        
    stage('Install dependencies') {
      steps {
        withNPM(npmrcConfig:'my-custom-npmrc') {
            echo "Performing npm build..."
            sh 'npm install'
      }
    }
     
    stage('Test') {
      steps {
         sh 'npm test'
      }
    }      
  }
}